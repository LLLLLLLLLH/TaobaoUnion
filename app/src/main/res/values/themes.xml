<resources>
    <!-- Base application theme. -->
    <style name="Theme.TaobaoUnion" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/teal_700</item>
        <item name="colorPrimaryDark">@color/teal_200</item>
        <item name="colorOnPrimary">@color/red_200</item>
    </style>

    <style name="Theme.user" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>



    <declare-styleable name="AutoLooperStyle">
        <attr name="duration" format="integer"/>
    </declare-styleable>
    <declare-styleable name="FlowTextStyle">
        <attr name="horizontalSpace" format="dimension"/>
        <attr name="verticalSpace" format="dimension"/>
    </declare-styleable>
</resources>